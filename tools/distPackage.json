{
  "name": "mozarella-frontend",
  "version": "1.0.0",
  "description": "Frontend for the language learning app mozarella",
  "scripts": {
    "coveralls": "babel-node node_modules/istanbul/lib/cli cover node_modules/mocha/bin/_mocha -- --report lcovonly --require tools/testSetup.js --bail --recursive \"src/**/*.test.js\" && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "coverage": "babel-node node_modules/istanbul/lib/cli cover node_modules/mocha/bin/_mocha -- --report lcovonly --require tools/testSetup.js --bail --recursive \"src/**/*.test.js\"",
    "prestart": "node tools/startMessage.js",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter spec tools/testSetup.js  \"src/**/*.test.js\"",
    "testdebug": "mocha --reporter spec tools/testSetup.js  \"src/**/*.test.js\" --debug-brk",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test lint build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "node tools/distServer.js"
  },
  "dependencies": {
    "babel-polyfill": "6.8.0",
    "babel-preset-stage-2": "^6.13.0",
    "bootstrap": "3.3.6",
    "chai": "^3.5.0",
    "cheerio": "^0.20.0",
    "colors": "1.1.2",
    "compression": "^1.6.1",
    "es6-promise": "^4.0.5",
    "express": "4.13.4",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "2.2.3",
    "mocha": "^2.4.5",
    "open": "0.0.5",
    "react": "15.4.1",
    "react-addons-update": "^15.4.2",
    "react-dom": "15.4.1",
    "react-dropdown": "^1.2.1",
    "react-redux": "4.4.5",
    "react-router": "2.6.0",
    "react-router-redux": "4.0.4",
    "react-sound": "^0.5.2",
    "redux": "3.5.2",
    "redux-form": "^6.2.0",
    "redux-thunk": "2.0.1",
    "toastr": "2.1.2",
    "whatwg-fetch": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-plugin-istanbul": "^1.0.0-alpha",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "^6.8.0",
    "blanket": "git://github.com/alex-seville/blanket.git",
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "cross-env": "^1.0.7",
    "css-loader": "0.23.1",
    "enzyme": "2.2.0",
    "es6-promise": "^4.0.5",
    "eslint": "2.9.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "eventsource-polyfill": "0.9.6",
    "expect": "^1.19.0",
    "extract-text-webpack-plugin": "1.0.1",
    "fetch-mock": "^5.9.3",
    "file-loader": "0.8.5",
    "istanbul": "^1.0.0-alpha",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "8.5.0",
    "mocha": "^2.5.3",
    "nock": "^8.2.1",
    "npm-run-all": "1.8.0",
    "nyc": "^10.1.2",
    "react-addons-test-utils": "15.0.2",
    "react-test-renderer": "^15.4.2",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-mock-store": "^1.0.2",
    "redux-thunk": "^2.0.1",
    "rimraf": "2.5.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  }
}
